<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center">
        <rotate
            android:fromDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape android:shape="oval">
                <stroke
                    android:width="4dp"
                    android:color="#4E4B4B" />
            </shape>
        </rotate>
    </item>

</layer-list>