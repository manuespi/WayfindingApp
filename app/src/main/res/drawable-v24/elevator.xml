<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center">
        <rotate
            android:fromDegrees="0"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape android:shape="rectangle">
                <stroke
                    android:width="2dp"
                    android:color="#5F5D5D" />
            </shape>
        </rotate>
    </item>

</layer-list>